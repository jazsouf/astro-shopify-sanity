{
  "name": "astro-shopify-sanity",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "check": "astro check",
    "astro": "astro",
    "astro:dev": "astro dev",
    "dev": "run-p sanity:typegen astro:dev",
