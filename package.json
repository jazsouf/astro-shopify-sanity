    "@nanostores/persistent": "^1.0.0",
    "@sanity/astro": "^3.2.6",
    "@sanity/client": "^7.3.0",
    "@sanity/color-input": "^4.0.3",
    "@sanity/icons": "^3.7.0",
    "@sanity/ui": "^2.15.18",
    "@sanity/vision": "^3.89.0",
    "@shopify/hydrogen-react": "^2025.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/vite": "^4.1.7",
